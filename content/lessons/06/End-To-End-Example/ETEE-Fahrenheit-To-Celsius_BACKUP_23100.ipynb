    "## function f2c\n",
    "# argument: temp in F\n",
    "# return temp in C\n",
    "\n",
    "def f2c( temp_f ):\n",
    "    temp_c = (temp_f - 32) * 5/9\n",
    "    return temp_c\n",
    "\n",
    "print(\"f2c when f = 32 expect f2c(32) = 0 actual \", f2c(32))\n",
    "print(\"f2c when f = 212 expect 100 c actual\", f2c(212))"
